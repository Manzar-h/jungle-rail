<tr>
  <td>
    <%= image_tag product.image.tiny.url , alt: product.name %>
    <%= product.name %>
    <%= product.description %>
  </td>
  <td><%= entry[:quantity] %></td>
  <td><%= humanized_money_with_symbol product.price %></td>
  <td><%= humanized_money_with_symbol entry[:quantity]*product.price %></td>
</tr>